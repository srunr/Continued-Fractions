project Contfrac is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("contfrac.adb", "contfrac2.adb", "contfrac3.adb", "contfrac4.adb", "contfrac5.adb");

   package Compiler is
      for Switches ("ada") use ("-gnatNp", "-O3", "-march=native", "-mtune=native", "-funroll-loops", "-ffast-math");
   end Compiler;

end Contfrac;

